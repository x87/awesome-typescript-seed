{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "indent": [true, "spaces"],
    "quotemark": [true, "single"],
    "trailing-comma": [
      true,
      {
        "objects": "always",
        "arrays": "always",
        "functions": "never",
        "typeLiterals": "ignore"
      }
    ]
  },
  "rulesDirectory": []
}
